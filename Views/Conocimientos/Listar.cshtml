@model List<ConocimientosModel>
@{
    ViewData["Title"] = "Listar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card col-8">
   @*  <div class="card-header">
        Manejo
    </div> *@
    <div class="card-body">

        <table class="table ">
            <thead>
                <tr>
                    <th>Conocimientos en:</th>
                    
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td style="">@Html.DisplayFor(modelItem=>item.Conocimiento)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

